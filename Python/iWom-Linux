LOG="/home/pi/iWom-inator/iWom-inator.log"
EMAIL_LOG="/home/pi/iWom-inator/iWom-sendemail.log"
echo "Entering iWom-inator" > $LOG
date >> $LOG
cd /home/pi/iWom-inator/iWom-inator-Services
/usr/bin/python3 iWom-update.py chrome >> $LOG
cd /home/pi/iWom-inator/iWom-inator-Delivery
/usr/bin/python3 iWom-update.py chrome >> $LOG
date >> $LOG
echo "Exiting iWom-inator" >> $LOG
cd ..
echo "Sending Log file by Telegram & email" >> $LOG
cat $LOG | telegram-send --stdin
/usr/bin/python3 SendEmailOutlook.py > $EMAIL_LOG
